{
  "name": "resume-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "concurrently \"npm run db:local\" \"next dev\"",
    "dev:remote": "concurrently \"npm run db:remote\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:local": "./scripts/update-env.sh local && turso dev --db-file resume-builder.db",
    "db:remote": "./scripts/update-env.sh remote"
  },
  "dependencies": {
    "@libsql/client": "^0.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "lucide-react": "^0.323.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
